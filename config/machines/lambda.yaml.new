---
id: lambda
node:
  max: 1
gpu:
  number: 0
  max: 0
cpu:
  cores: 12
  simd: AVX2_256
run:
  command: |
   cp TPRFILEPATH TPRFILE.tpr
   mpirun -np NTMPI mdrun_mpi -deffnm TPRFILE -ntomp NTOMP -resethway -maxh 0.1 -noconfout -pin on
gromacs:
  version: 2016.4
  path: /usr/local/gromacs/2016.4/bin/GMXRC
